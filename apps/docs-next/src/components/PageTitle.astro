---
    import "@studs/ui/display/chip";
    const {component: comp, category, figmaURL, status, title, description} = Astro.props;
    const version = '1.0.0-beta.8'; // STORE THIS IN A GLOBAL STORE: https://docs.astro.build/en/recipes/sharing-state-islands/#why-nano-stores
    const component = comp.split('_')[1];
    let githubURL = `https://github.com/studs-ui/studs/tree/main/packages/ui/src/components/${category}/${component}`;
    let storybookURL = (import.meta.env.MODE === 'development' ? 'http://localhost:6002' : window.location.origin + '/storybook') + `/?path=/docs/studs-${category}-${component}--docs`;
    let pakaURL;
    let unpkgURL = `https://unpkg.com/@studs/ui@${version}/dist/components/${category}/${component}.js`
---
<style scoped>
    .-icons {
        display: flex;
        gap: 0.75em;
        align-items: center;
    }

    .-icons studs-chip {
        text-transform: capitalize;
    }

    .-icons svg.icon {
        margin-top: 0rem;
    }

    h1.component.-heading {
        font-weight: 700;
        margin-top: 0;
        margin-block-start: 1.25em;

		font-size: var(--sl-text-h1);
		line-height: var(--sl-line-height-headings);
		color: var(--text-color);
    }
</style>
