{
  "name": "@studs/react",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "@studs/styles",
    "*.css"
  ],
  "exports": {
    "./styles": "./dist/index.css",
    "./studs-base.css": "./dist/index.css",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": {
      "require": "./dist/components/Studs*.cjs",
      "import": "./dist/components/Studs*.js",
      "types": "./dist/components/Studs*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "vite build --watch",
    "lint": "tsc",
    "preview": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@lit/react": "^1.0.4",
    "@studs/styles": "workspace:1.0.1",
    "@studs/ui": "workspace:1.0.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0 <19.0.0",
    "react-dom": ">=17.0.0 <19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "esbuild-envfile-plugin": "^1.0.5",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  }
}
